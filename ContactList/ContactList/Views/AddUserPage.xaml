<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ContactList.Views.AddUserPage"
             >
        <StackLayout Padding="10">
        <Label Style="{StaticResource titleLabelStyle}"/>
        <Entry x:Name="newName" Placeholder="Name" Text="{Binding Name}"/>
            <Entry x:Name="newNumber" Placeholder="Number" Text="{Binding Number}"/>
        <Button Style="{StaticResource buttonStyle}" Command="{Binding RegisterCommand}"/>
    </StackLayout>
</ContentPage>